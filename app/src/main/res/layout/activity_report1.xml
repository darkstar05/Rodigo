<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgy"
    tools:context=".Report0">

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="137dp"
        android:layout_height="55dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="166dp"
        android:layout_height="51dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="69dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/spinner3"
        app:layout_constraintEnd_toEndOf="parent" />

    <Spinner
        android:id="@+id/spinner3"
        android:layout_width="248dp"
        android:layout_height="46dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="14dp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <MultiAutoCompleteTextView
        android:id="@+id/write"
        android:layout_width="339dp"
        android:layout_height="114dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="29dp"
        android:background="@drawable/edit_round"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="115dp"
        android:layout_height="23dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="102dp"
        android:text="District"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="88dp"
        android:layout_height="23dp"
        android:layout_marginEnd="117dp"
        android:layout_marginBottom="12dp"
        android:text="Area"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/spinner2"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="117dp"
        android:layout_height="17dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="38dp"
        android:text="Road"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner" />

    <Button
        android:id="@+id/fab"
        android:layout_width="65dp"
        android:layout_height="50dp"
        android:layout_gravity="bottom|end"
        android:layout_margin="16dp"

        android:layout_marginTop="220dp"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="131dp"
        android:background="@drawable/cameraa"
        app:layout_constraintBottom_toTopOf="@+id/submit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/lat"
        app:layout_constraintTop_toBottomOf="@+id/write" />

    <Button
        android:id="@+id/submit"
        android:layout_width="127dp"
        android:layout_height="45dp"
        android:layout_marginStart="131dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="153dp"
        android:background="@drawable/report2"
        android:fontFamily="@font/bowlby_one"
        android:text="SUBMIT"
        android:textColor="#F7F1F1"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lng" />

    <EditText
        android:id="@+id/lat"
        android:layout_width="172dp"
        android:layout_height="46dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="35dp"
        android:background="@drawable/edit_round"
        android:ems="10"
        android:hint="  Latitude"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/write" />

    <EditText
        android:id="@+id/lng"
        android:layout_width="172dp"
        android:layout_height="46dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/edit_round"
        android:ems="10"
        android:hint="  Longitude"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lat" />
    <Button
        android:id="@+id/logout"
        android:layout_width="129dp"
        android:layout_height="36dp"
        android:layout_marginStart="259dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="640dp"
        android:background="@drawable/logout"


        android:onClick="login"
        android:textColor="#F7F6F6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ProgressBar
        android:id="@+id/progressBar2"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="209dp"
        android:layout_marginBottom="13dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/spinner3"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>